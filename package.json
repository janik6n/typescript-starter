  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch & node --watch-preserve-output --watch-path=./dist dist/index.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:cov": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest --watch",
    "lint": "eslint .",
    "clean": "rm -rf ./dist && mkdir ./dist",
    "build": "npm run clean && node ./build.js",
    "build:container": "echo \"TODO: Not implemented yet\" && exit 1",
    "start": "node ./dist/index.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
